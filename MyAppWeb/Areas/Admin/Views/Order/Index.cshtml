<h1> List of orders</h1>
<div class="container p-4">
    <div class="row p-3">
        <div class="col-6">List Of Products</div>

        <div class="col-6">
            <a asp-controller="Order" asp-action="Index" asp-route-status="pending">Pending</a> ||
            <a asp-controller="Order" asp-action="Index" asp-route-status="approved">Approved</a>
        </div>
    </div>
</div>
<table id="myTable" class="table table-bordered" style="width:100%">
    <thead>
        <tr>
            <th>Name</th>
            <th>Phone Number</th>
            <th>Order Status</th>
            <th>Order Total</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td></td>

        </tr>
    </tbody>
</table>
